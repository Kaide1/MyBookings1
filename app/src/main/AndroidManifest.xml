<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mybookings">
<uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyBookings">
        <activity
            android:name=".UserProfile"
            android:exported="false" />

        <activity
            android:name=".Settings"
            android:exported="false" />
        <activity
            android:name=".Payment_Notification"
            android:exported="false" />
        <activity
            android:name=".Payment"
            android:exported="false" />
        <activity android:name=".EventDetails" />
        <activity android:name=".Fragments.Homepage" />
        <activity android:name=".Registration" />
        <activity android:name=".Login" android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>